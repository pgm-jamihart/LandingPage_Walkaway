{
  "name": "my-walking-app",
  "version": "1.0.0",
  "description": "een landingspagina voor desktop gebruik om promotie te maken voor de  wandel/hiking app",
  "main": "eleventy.js",
  "scripts": {
    "eleventy": "npx @11ty/eleventy",
    "sass": "sass --style=compressed src/style/main.scss src/main.css",
    "watch:sass": "npm run sass -- --watch",
    "watch:dev": "npx @11ty/eleventy --serve",
    "start": "npm-run-all sass --parallel watch:*",
    "build": "npm run eleventy && npm run sass"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pgmgent-2021-uip/exercises-pgm-jamihart.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pgmgent-2021-uip/exercises-pgm-jamihart/issues"
  },
  "homepage": "https://github.com/pgmgent-2021-uip/exercises-pgm-jamihart#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "npm-run-all": "^4.1.5"
  }
}
